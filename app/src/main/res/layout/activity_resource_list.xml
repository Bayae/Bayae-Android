<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.dudes.dexin.bayae.RankList.ResourceList">

    <include
        android:id="@+id/include_lib_title"
        layout="@layout/lib_title_bar"
        android:layout_width="fill_parent"
        android:layout_height="65dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"/>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="34dp"
        android:layout_marginStart="34dp"
        android:text="TextView"
        android:layout_toRightOf="@+id/textView5"
        android:layout_toEndOf="@+id/textView5"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignTop="@+id/textView5" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginTop="24dp"
        android:text="TextView"
        android:layout_marginLeft="13dp"
        android:layout_marginStart="13dp"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/view4"
        android:layout_alignStart="@+id/view4" />

    <View
        android:layout_marginLeft="5dip"
        android:layout_marginRight="5dip"
        android:background="#9b000000"
        android:layout_width="fill_parent"
        android:layout_height="0.2dip"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="20dp"

        android:id="@+id/view4" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignBottom="@+id/textView2"
        android:layout_alignEnd="@+id/view4"
        android:layout_alignRight="@+id/view4"
        android:layout_alignTop="@+id/textView2"
        android:layout_marginEnd="19dp"
        android:layout_marginRight="19dp"
        android:background="@drawable/heart" />




</RelativeLayout>
